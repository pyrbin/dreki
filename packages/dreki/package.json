{
  "name": "dreki",
  "version": "0.0.10",
  "description": "🐉 An Entity-Component System library written in Typescript",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "out/mod.js",
  "types": "out/mod.d.ts",
  "exports": {
    ".": "./out/mod.js",
    "./package.json": "./package.json",
